{
    "RTB":
    {
        "Client":
        {
            "Version": "2.5"
        }
    },
    "Hooks":
    {
        "beforeRTBRequest":
        {
            "Generate ID":
            {
                "Service": "Advertisement.RTB.SSP.Hooks.ID"
            },
            "Setup Impression":
            {
                "Service": "Advertisement.RTB.SSP.Hooks.Impression"
            },
            "Setup Device":
            {
                "Service": "Advertisement.RTB.SSP.Hooks.Device"
            },
            "Setup Site":
            {
                "Service": "Advertisement.RTB.SSP.Hooks.Site"
            },
            "Setup User":
            {
                "Service": "Advertisement.RTB.SSP.Hooks.User"
            },
            "Merge Bid":
            {
                "Service": "Advertisement.RTB.SSP.Hooks.Bid"
            },
            "Convert Request Data to JSON":
            {
                "Service": "Advertisement.RTB.SSP.Hooks.DataToJSON"
            }
        },
        "afterRTBRequest":
        {
            "Select Bids": 
            {
                "Service": "Advertisement.RTB.SSP.Client.Comparator",
                "Call": 
                {
                    "RTB":
                    {
                        "SSP":
                        {
                            "Client":
                            {
                                "Comparator":
                                {
                                    "Strategy": "Valuable"    
                                }
                            }
                        }
                    }
                }
            },
            "Send RTB Win Notice":
            {
                "Service": "Advertisement.RTB.SSP.Hooks.WinNotice"
            }
        }
    }
}