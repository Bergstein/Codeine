{
    "CURL":
    {
        "Connect Timeout": 4,
        "Overall Timeout": 8
    },
    "RTB":
    {
        "Client":
        {
            "Version": "2.5",
            "Impression":
            {
                "ID":
                {
                    "Mode": "Secure"
                }
            },
            "Request":
            {
                "ID":
                {
                    "Mode": "Secure"
                }
            }
        }
    },
    "Hooks":
    {
        "beforeRTBRequest":
        {
            "Generate Request ID":
            {
                "Service": "Advertisement.RTB.SSP.Hooks.GenerateRequestID",
                "Method": "beforeRTBRequest"
            },
            "Generate Impression ID":
            {
                "Service": "Advertisement.RTB.SSP.Hooks.GenerateImpressionID",
                "Method": "beforeRTBRequest"
            },
            "Fill Device Section":
            {
                "Service": "Advertisement.RTB.SSP.Hooks.FillDeviceSection",
                "Method": "beforeRTBRequest"
            },
            "Fill Site Section":
            {
                "Service": "Advertisement.RTB.SSP.Hooks.FillSiteSection",
                "Method": "beforeRTBRequest"
            },
            "Fill User Section":
            {
                "Service": "Advertisement.RTB.SSP.Hooks.FillUserSection",
                "Method": "beforeRTBRequest"
            }
        },
        "RTB":
        {
            "SSP":
            {
                "RequestGroup":
                {
                    "Executed":
                    {
                        "Convert Endpoints To Names":
                        {
                            "Service": "Advertisement.RTB.SSP.Hooks.ConvertEndpointsToNames",
                            "Method": "RTB.SSP.Request.Executed"
                        },
                        "Clean Empty Results":
                        {
                            "Service": "Advertisement.RTB.SSP.Hooks.CleanEmptyResults",
                            "Method": "RTB.SSP.Request.Executed"
                        },
                        "Compare Bids":
                        {
                            "Service": "Advertisement.RTB.SSP.Client.Comparator",
                            "Method": "RTB.SSP.Request.Executed"
                        }
                    }
                },
                "Winner":
                {
                    "Exists":
                    {
                        "Substitute Macroses":
                        {
                            "Service": "Advertisement.RTB.SSP.Hooks.SubstituteMacroses",
                            "Method": "RTB.SSP.Request.Executed"
                        },
                        "Send RTB Win Notice":
                        {
                            "Service": "Advertisement.RTB.SSP.Hooks.WinNotice",
                            "Method": "RTB.SSP.Winner.Exists"
                        }
                    }
                }
            }
        }
    }
}