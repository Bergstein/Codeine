{
    "Regex":
    {
        "Test App":
        {
            "Match": "^/test/(.+).ajax$",
            "Weight": 5,
            "Service": "Test",
            "Method": "Run.Test",
            "Zone": "Test",
            "Call":
            {
                "Context": "app",
                "ID": "$1"
            }
        },
        "Test / JSON":
        {
            "Match": "^/test/(.+)\\.json$",
            "Weight": 10,
            "Service": "Test",
            "Method": "Run.Test",
            "View":
            {
                "Renderer":
                {
                    "Service": "View.JSON",
                    "Method": "Render"
                },
                "JSON":
                {
                    "RAW": true
                }
            },
            "Call":
            {
                "ID": "$1"
            }
        },
        "Test":
        {
            "Match": "^/test/(.+)$",
            "Weight": 5,
            "Service": "Test",
            "Method": "Run.Test",
            "Zone": "Test",
            "Call":
            {
                "ID": "$1"
            }
        },
        "All Tests":
        {
            "Match": "^/test$",
            "Weight": 5,
            "Service": "Test",
            "Method": "List.All",
            "Zone": "Test"
        }
    }
}