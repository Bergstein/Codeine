{
    "Regex":
    {
        "Wiki TOC":
        {
            "Match": "^/wiki$",
            "Weight": 200,
            "Service": "Entity.List",
            "Method": "Do",
            "Call":
            {
                "Entity": "Wiki",
                "Template": "Tree",
                "Where":
                {
                    "Level": 1
                },
                "Custom Templates":
                {
                    "List": "Tree"
                }
            }
        },
        "List Wiki":
        {
            "Match": "^/wiki/list$",
            "Weight": 150,
            "Service": "Entity.List",
            "Method": "Do",
            "Call":
            {
                "Entity": "Wiki",
                "No Where": true
            }
        },
        "Update wiki":
        {
            "Match": "/wiki/update/(.+)",
            "Weight": 150,
            "Service": "Entity.Update",
            "Method": "Do",
            "Call":
            {
                "Entity": "Wiki",
                "Form":
                {
                    "Layout": "Field"
                },
                "Where": "$1"
            }
        },
        "Delete wiki":
        {
            "Match": "/wiki/delete/(.+)",
            "Weight": 150,
            "Service": "Entity.Delete",
            "Method": "Do",
            "Call":
            {
                "Entity": "Wiki",
                "Where": "$1"
            }
        },
        "Create wiki with parent":
        {
            "Match": "/wiki/create/(.+)",
            "Weight": 150,
            "Service": "Entity.Create",
            "Method": "Do",
            "Call":
            {
                "Entity": "Wiki",
                "Data":
                {
                    "Parent": "$1"
                }
            }
        },
        "Create wiki":
        {
            "Match": "/wiki/create",
            "Weight": 150,
            "Service": "Entity.Create",
            "Method": "Do",
            "Call":
            {
                "Entity": "Wiki"
            }
        },
        "Show Wiki":
        {
            "Match": "^/wiki/(.+)$",
            "Weight": 100,
            "Service": "Entity.Show.Static",
            "Method": "Do",
            "Call":
            {
                "Entity": "Wiki",
                "Where":
                {
                    "SEO.Slug": "$1"
                }
            }
        }
    }
}