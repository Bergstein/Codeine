{
    "Robots":
    {
        "Crawl-delay": 0,
        "Crawlers":
        {
            "*": "/go/",
            "AhrefsBot": "/"
        }
    },
    "Hooks":
    {
        "beforeSEORobotsGenerate":
        {
            "Check Domain Is Proxy":
            {
                "Service": "SEO.Robots.Hooks.CheckDomain",
                "Method": "Do"
            }
        }
    }
}