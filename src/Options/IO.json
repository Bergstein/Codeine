{
    "Mixins":
    {
        "Logs": "IO.Mixins.Logs2File",
        "Behaviours": "IO.Mixins.Behaviours2Redis",
        "View": "View.IO",
        "Test": "Test.IO",
        "Metric": "Metric.IO.MongoRedis"
    },
    "Contract!":
    {
        "Methods":
        {
            "Read":
            {
                "Behaviours":
                {
                    "Cached":
                    {
                        "Enabled": false,
                        "TTL": 60,
                        "Keys": ["Storage", "Scope", "Where", "Time"]
                    }
                },
                "Input":
                {
                    "Required":
                    {
                        "Storage":
                        {
                            "Description": "",
                            "Type": "Literal.String"
                        }
                    }
                },
                "Output":
                {
                    "Mode": "Result"
                },
                "Fallback": null
            },
            "Count":
            {
                "Behaviours":
                {
                    "Cached":
                    {
                        "Enabled": false,
                        "TTL": 60,
                        "Keys": ["Storage", "Scope", "Where", "Time"]
                    }
                },
                "Input":
                {
                    "Required":
                    {
                        "Storage":
                        {
                            "Description": "",
                            "Type": "Literal.String"
                        }
                    }
                },
                "Output":
                {
                    "Mode": "Result"
                },
                "Fallback": null
            }
        }
    },
    "Verbosity":
    {
        "Not found": 6
    },
    "Storages":
    {
        "Output":
        {
            "Driver": "IO.Print"
        },
        "Primary":
        {
            "Driver": "IO.Storage.Memory",
            "Description": "Default storage for all important data"
        },
        "Secondary":
        {
            "Driver":"IO.Storage.FileSystem",
            "Directory":"Data",
            "Format": "Formats.JSON",
            "Postfix":".json",
            "Description": "Default storage for all immutable important data"
        },
        "Permanent":
        {
            "Description": "Storage for source-based data",
            "Driver":"IO.Storage.FileSystem",
            "Directory":"Data",
            "Format": "Formats.JSON",
            "Postfix":".json"
        },
        "Cookie":
        {
            "Driver":"IO.Storage.Cookie",
            "TTL": 86400,
            "Path": "/",
            "Secure": false,
            "HTTP Only": true
        },
        "Options":
        {
            "Driver":"IO.Storage.FileSystem",
            "Directory":"Options",
            "Format": "Formats.JSON",
            "Postfix":".json"
        },
        "EMail":
        {
            "Driver": "IO.Transport.Mail.SMTP",
            "From": "Unconfigured Codeine Framework <noreply@codeine-framework.org>"
        },
        "Upload":
        {
            "Driver": "IO.Storage.FileSystem",
            "Directory": "Data",
            "Verbosity":
            {
                "Not found": 5
            }
        },
        "Static Sitemaps":
        {
            "Driver": "IO.Storage.FileSystem",
            "Directory": "Public/sitemap",
            "Postfix": ".xml"
        },
        "Uploader":
        {
            "Driver": "IO.Storage.FileSystem",
            "Directory": "Public/uploads",
            "Verbosity":
            {
                "Not found": 5
            }
        },
        "Web":
        {
            "Driver": "IO.Network.HTTP.CURL",
            "Scope": ""
        },
        "HTTP Journal":
        {
            "Driver":"IO.Storage.FileSystem",
            "Directory":"/var/log/codeine/",
            "Postfix": ".json",
            "Format": "Formats.JSON"
        },
        "Keys":
        {
            "Driver": "IO.Storage.FileSystem",
            "Directory": "Keys"
        }
    },
    "Hooks":
    {
        "beforeIOWrite":
        {
            "-Journaling for writes":
            {
                "Service": "IO.Hooks.Journal",
                "Method": "beforeIOWrite"
            }
        }
    }
}
