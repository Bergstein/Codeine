{
    "Hooks":
    {
        "beforeCreateGet":
        {
            "Store URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "StoreURL"
            }
        },
        "afterCreatePost":
        {
            "Log Creation":
            {
                "Service": "IO.Log",
                "Method": "Hook",
                "Call":
                {
                    "Message": "$Entity $Data.0.ID ($Data.0.Title) created by User $Session.User.ID ($Session.User.EMail)",
                    "Channel": "Management",
                    "Verbose": 5
                }
            },
            "Redirect":
            {
                "Service": "System.Interface.HTTP",
                "Method": "Redirect",
                "Call":
                {
                    "Redirect": "/$Slug.Entity/$Data.0.ID"
                }
            }
        },
        "beforeUpdateGet":
        {
            "Store URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "StoreURL"
            }
        },
        "afterUpdatePost":
        {
            "Restore URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "RestoreURL"
            }
        },
        "beforeSetDo":
        {
            "Store URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "StoreURL"
            }
        },
        "afterSetDo":
        {
            "Restore URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "RestoreURL"
            }
        },
        "beforeIncreaseDo":
        {
            "Store URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "StoreURL"
            }
        },
        "afterIncreaseDo":
        {
            "Restore URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "RestoreURL"
            }
        },
        "beforeDeleteGet":
        {
            "Store URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "StoreURL"
            }
        },
        "afterDeletePost":
        {
            "Log Deletion":
            {
                "Service": "IO.Log",
                "Method": "Hook",
                "Call":
                {
                    "Message": "$Entity $Data.0.ID ($Data.0.Title) deleted by User $Session.User.ID ($Session.User.EMail)",
                    "Channel": "Management",
                    "Verbose": 5
                }
            },
            "Restore URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "RestoreURL"
            }
        },
        "beforeTouch":
        {
            "Store URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "StoreURL"
            }
        },
        "afterTouch":
        {
            "Restore URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "RestoreURL"
            }
        }
    }
}