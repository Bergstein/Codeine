{
    "Suites":
    {
        "Filter types":
        {
            "Float":
            {
                "Run":
                {
                    "Service": "Security.Filter",
                    "Call": 
                    {
                        "Data":
                        {
                            "Value": " klmn 90,33asd"
                        },
                        "Filter":
                        {
                            "Name": "Value",
                            "Type": "Float"
                        }
                    }
                },
                "Result":
                {
                    "Extract": "Data.Value"
                },
                "Assert":
                {
                    "Equal": "90,33"
                }
            },
            "Integer":
            {
