{
    "Suites":
    {
        "JSON":
        {
            "Simple Encode":
            {
                "Run":
                {
                    "Service": "Formats.YAML",
                    "Method":  "Write",
                    "Call":
                    {
                        "Value":
                        {
                            "A": 1,
                            "B": "X",
                            "C": [0, 1, 2]
                        },
                        "Flags": 352
                    }
                },
                "Assert":
                {
                    "Print": true,
                    "Empty": false,
                    "Equal": "{\"A\":1,\"B\":\"X\",\"C\":[0,1,2]}"
                }
            },
            "Simple Decode":
            {
                "Run":
                {
                    "Service": "Formats.YAML",
                    "Method":  "Read",
                    "Call":
                    {
                        "Value": "{\"A\":1,\"B\":\"X\",\"C\":[0,1,2]}"
                    }
                },
                "Assert":
                {
                    "Print": true,
                    "Empty": false,
                    "Equal":
                    {
                        "A": 1,
                        "B": "X",
                        "C": [0, 1, 2]
                    }
                }
            }
        }
    }
}