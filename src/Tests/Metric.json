{
    "Mixins":
    [
        "Metric.Mixins.Clear"
    ],
    "Suites":
    {
        "Positive Suite":
        {
            "Set Random Value for Test Metric":
            {
                "Run":
                {
                    "Service": "Metric.Set",
                    "Method": "Do",
                    "Call":
                    {
                        "Metric":
                        {
                            "Type": "Test.Metric",
                            "Dimensions":
                            {
                                "A": 1,
                                "B": "X"
                            },
                            "Resolutions": [60, 3600, 86400],
                            "Value":
                            {
                                "Service": "Security.Entropy",
                                "Method": "Get",
                                "Call":
                                {
                                    "Min": 100,
                                    "Max": 1000
                                }
                            }
                        }
                    }
                },
                "Assert":
                {
                    "Print": true
                }
            },
            "Get Values for Test.Metric.A1.BX with R3600":
            {
                "Run":
                {
                    "Service": "Metric.Get",
                    "Method": "Row",
                    "Call":
                    {
                        "Metric":
                        {
                            "Type": "Test.Metric",
                            "Dimensions":
                            {
                                "A": 1,
                                "B": "X"
                            },
                            "Resolutions": [60]
                        }
                    }
                },
                "Assert":
                {
                    "Print": true
                }
            },
            "Get Sum for Test.Metric.A1.BX with R3600":
            {
                "Run":
                {
                    "Service": "Metric.Calc",
                    "Method": "Sum",
                    "Call":
                    {
                        "Metric":
                        {
                            "Type": "Test.Metric",
                            "Dimensions":
                            {
                                "A": 1,
                                "B": "X"
                            },
                            "Resolutions": [60]
                        }
                    }
                },
                "Assert":
                {
                    "Print": true
                }
            }
        }
    }
}