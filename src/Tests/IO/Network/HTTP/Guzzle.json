{
    "Suites":
    {
        "Guzzle":
        {
            "Single GET":
            {
                "Run":
                {
                    "Service": "IO.Network.HTTP.Guzzle",
                    "Method": "Read",
                    "Call": 
                    {
                        "Where":
                        {
                            "ID": "$HTTP.Proto$HTTP.Host/api/3.0/json/AdRam/GetProtectedURLByURL"
                        },
                        "Data": 
                        {
                            "URL": "$HTTP.Proto$HTTP.Host"
                        }
                    }
                },
                "Assert":
                {
                    "Print": true
                }
            },
            "Multiple GET":
            {
                "Run":
                {
                    "Service": "IO.Network.HTTP.Guzzle",
                    "Method": "Read",
                    "Call": 
                    {
                        "Where":
                        {
                            "ID": [
                                "$HTTP.Proto$HTTP.Host/api/3.0/json/AdRam/GetProtectedURLByURL",
                                "$HTTP.Proto$HTTP.Host/api/3.0/json/AdRam/GetProtectedURLByURL",
                                "$HTTP.Proto$HTTP.Host/api/3.0/json/AdRam/GetProtectedURLByURL"
                            ]
                        },
                        "Data": 
                        [
                            {"URL": "$HTTP.Proto$HTTP.Host"},
                            {"URL": "$HTTP.Proto$HTTP.Host"},
                            {"URL": "$HTTP.Proto$HTTP.Host"}
                        ]
                    }
                },
                "Assert":
                {
                    "Print": true
                }
            }
        }
    }
}