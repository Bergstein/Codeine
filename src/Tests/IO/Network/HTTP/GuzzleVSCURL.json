{
    "Suites":
    {
        "Guzzle VS CURL":
        {
            "Guzzle (stream backend): Single GET":
            {
                "Run":
                {
                    "Service": "IO.Network.HTTP.Guzzle",
                    "Method": "Read",
                    "Call": 
                    {
                        "Where":
                        {
                            "ID": "$HTTP.Proto$HTTP.Host/api/3.0/json/AdRam/GetProtectedURLByURL"
                        },
                        "Data": 
                        {
                            "URL": "$HTTP.Proto$HTTP.Host"
                        },
                        "Guzzle":
                        {
                            "Client":
                            {
                                "Backend": "Stream"
                            }
                        }
                    }
                },
                "Assert":
                {
                    "Print": true
                }
            },
