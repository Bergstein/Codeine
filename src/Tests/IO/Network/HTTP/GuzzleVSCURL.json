{
    "Suites":
    {
        "Guzzle VS CURL":
        {
            "Guzzle (stream backend): Single GET":
            {
                "Run":
                {
                    "Service": "IO.Network.HTTP.Guzzle",
                    "Method": "Read",
                    "Call": 
                    {
                        "Where":
                        {
                            "ID": "https://google.com"
                        },
                        "Guzzle":
                        {
                            "Client":
                            {
                                "Backend": "Stream"
                            }
                        }
                    }
                },
                "Assert":
                {
                    "Print": true
                }
            },
