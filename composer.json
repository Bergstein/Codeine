{
    "name": "trickyplan/codeine",
    "version": "46.0.2.0",
    "minimum-stability": "dev",
    "description": "Esoteric framework: one class, shitload of anonymous functions, zero documentation. Still good though.",
    "keywords": [
        "framework",
        "closure",
        "causes ptsd"
    ],
    "homepage": "https://github.com/Bergstein/Codeine",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dmitry Bergstein",
            "email": "bergstein@trickyplan.com"
        }
    ],
    "bin": [
        "bin/codeine"
    ],
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "vendor-dir": "src/vendor"
    },
    "require": {
        "php": "^8.0",
        "mongodb/mongodb": "^1.7",
        "ext-mongodb": "^1.8",
        "ext-imagick": "^3.4.4",
        "phpmailer/phpmailer": "^6.1"
    }
}